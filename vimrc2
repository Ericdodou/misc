set nocompatible
filetype off


filetype plugin indent on

set ai
set history=100
set ruler
set hlsearch
colo desert

syntax on

filetype indent on
set number
set ic
set lbr
filetype plugin indent on
set tabstop=2 softtabstop=0 expandtab shiftwidth=2 smarttab
set backspace=2
set whichwrap+=<,>,h,l,[,]
